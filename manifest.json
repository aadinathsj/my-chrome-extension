{
    "manifest_version": 3,
    "name": "Website Restrictor",
    "version": "1.0",
    "description": "Notifies users if they are on a restricted website.",
    "permissions": [
      "activeTab",
      "storage",
      "notifications",
      "tabs"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
  